<script type="text/html" id="li-selecticon-outer">
    <button ref="buttonEl" readonly="true"
        role="combobox" aria-expanded="false"
        data-bind="attr: {
                aria-owns: dropdown.id,
                aria-activedescendant: dropdown.id + '-item-' + getIndexOfValue(getValue()),
                disabled: disabled() ? 'disabled' : null
            },
            css: {disabled: disabled},
            value: getValue()"></button>
</script>
